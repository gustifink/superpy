{
  "$schema": "https://superpy.io/schemas/ram_map.json",
  "game": "Super Mario World",
  "region": "USA",
  "version": "1.0",
  "contributors": ["SuperPy Team"],
  "addresses": {
    "player": {
      "x_position": {
        "address": "0x0094",
        "size": 2,
        "type": "uint16_le",
        "description": "Mario's X position in the level"
      },
      "y_position": {
        "address": "0x0096",
        "size": 2,
        "type": "uint16_le",
        "description": "Mario's Y position in the level"
      },
      "coins": {
        "address": "0x0DBF",
        "size": 1,
        "type": "uint8",
        "description": "Current coin count (0-99)"
      },
      "lives": {
        "address": "0x0DBE",
        "size": 1,
        "type": "uint8",
        "description": "Lives remaining"
      },
      "powerup_state": {
        "address": "0x0019",
        "size": 1,
        "type": "uint8",
        "description": "0=Small, 1=Big, 2=Cape, 3=Fire"
      },
      "is_on_ground": {
        "address": "0x13EF",
        "size": 1,
        "type": "bool",
        "description": "True if Mario is standing on ground"
      }
    },
    "level": {
      "current_world": {
        "address": "0x13BF",
        "size": 1,
        "type": "uint8",
        "description": "Current world number (0-indexed)"
      },
      "current_level": {
        "address": "0x13BF",
        "size": 1,
        "type": "uint8",
        "description": "Current level within world"
      },
      "time_remaining": {
        "address": "0x0F31",
        "size": 2,
        "type": "uint16_le",
        "description": "Time remaining in frames"
      },
      "score": {
        "address": "0x0F34",
        "size": 4,
        "type": "uint32_le",
        "description": "Current score"
      }
    },
    "game_state": {
      "is_paused": {
        "address": "0x13D4",
        "size": 1,
        "type": "bool",
        "description": "True if game is paused"
      },
      "is_dying": {
        "address": "0x0071",
        "size": 1,
        "type": "bool",
        "description": "True if Mario is in death animation"
      }
    }
  }
}
